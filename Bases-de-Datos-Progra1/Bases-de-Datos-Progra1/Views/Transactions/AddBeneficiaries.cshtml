

@{
    ViewData["Title"] = "Add Beneficiaries";
}



<div class="container">
    <div class="row">
        <div class="col-md-4 col-md-offset-4">
            <div class="login-panel panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">Ingrese sus datos</h3>
                </div>
                <div class="panel-body">
                    <fieldset>
                        <div class="form-group">
                            <i class="fas fa-user"></i> <br> <input class="form-control" placeholder="Nombre" name="usuario" type="text" autofocus id="txtUsuario" required>
                        </div>
                        <div class="form-group">
                            <i class="fas fa-id-card"></i> <br> <input class="form-control" placeholder="Identificacion" name="identificacion" type="text" autofocus id="txtIdentificacion" required>
                        </div>
                        <div class="form-group">
                            <i class="fas fa-at"></i> <br> <input class="form-control" placeholder="Email" name="email" type="email" autofocus id="txtEmail" required>
                        </div>
                        <div class="form-group">
                            <i class="fas fa-user-friends"></i> <br> <input class="form-control" placeholder="Parentesco" name="password" type="password" id="txtPassword" required>
                        </div>
                        <div class="form-group">
                            <i class="fas fa-percentage"></i> <br> <input class="form-control" placeholder="Porcentaje de Beneficio" name="porcentage" type="number" id="intPorcentage" required max="100" min="0">
                        </div>
                        <div class="form-group">
                            <i class="fas fa-phone"></i> <br> <input class="form-control" placeholder="Numero" name="numero" type="tel" id="intNumber" required>
                        </div>
                        <div class="form-group">
                            <i class="fas fa-calendar-week"></i> <br> <input class="form-control" name="date" type="date" id="date" required>
                        </div>
                        <div>
                            <button id="btnIngresar" class="btn btn-lg btn-success btn-block">Añadir</button>
                        </div>
                        <!-- Change this to a button or input when using this as a form -->

                    </fieldset>
                </div>
            </div>
        </div>
    </div>
</div>